{
  "AsyncRedux exports": {
    "prefix": "arexports",
    "description": "AsyncRedux exports",
    "body": [
      "export '${TM_FILENAME_BASE}_page.dart';",
      "export '${TM_FILENAME_BASE}_page_connector.dart';",
      "export '${TM_FILENAME_BASE}_view_model.dart';",
      "export '${TM_FILENAME_BASE}_view_model_factory.dart';",
      "$0"
    ]
  },
  "AsyncRedux page": {
    "prefix": "arpage",
    "description": "AsyncRedux page",
    "body": [
      "import 'package:flutter/material.dart';",
      "",
      "class ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/} extends StatelessWidget {",
      "  @override",
      "  Widget build(BuildContext context) => ${2:Container()};",
      "}",
      "$0"
    ]
  },
  "AsyncRedux page connector": {
    "prefix": "arpageconnector",
    "description": "AsyncRedux page connector",
    "body": [
      "import 'package:flutter/material.dart';",
      "import 'package:async_redux/async_redux.dart';",
      "",
      "import '${TM_FILENAME_BASE/(.*)_page_connector/$1/}.dart';",
      "",
      "class ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/} extends StatelessWidget {",
      "  @override",
      "  Widget build(BuildContext context) => StoreConnector<${1:St}, ${TM_FILENAME_BASE/(.*)_page_connector/${1:/pascalcase}ViewModel/}>(",
      "        vm: () => ${TM_FILENAME_BASE/(.*)_page_connector/${1:/pascalcase}ViewModelFactory/}(),",
      "        builder: (context, viewModel) => ${TM_FILENAME_BASE/(.*)_page_connector/${1:/pascalcase}Page/}($2),",
      "      );",
      "}",
      "$0"
    ]
  },
  "AsyncRedux view model": {
    "prefix": "arviewmodel",
    "description": "AsyncRedux view model",
    "body": [
      "import 'package:async_redux/async_redux.dart';",
      "",
      "class ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/} extends Vm {",
      "  ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}($2) : super(equals: [$3]);",
      "}",
      "$0"
    ]
  },
  "AsyncRedux view model factory": {
    "prefix": "arviewmodelfactory",
    "description": "AsyncRedux view model factory",
    "body": [
      "import 'package:async_redux/async_redux.dart';",
      "",
      "import '${TM_FILENAME_BASE/(.*)_view_model_factory/$1/}.dart';",
      "",
      "class ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/} extends VmFactory<${1:St}, ${TM_FILENAME_BASE/(.*)_view_model_factory/${1:/pascalcase}PageConnector/}> {",
      "  @override",
      "  Vm fromStore() => ${TM_FILENAME_BASE/(.*)_view_model_factory/${1:/pascalcase}ViewModel/}($2);",
      "}",
      "$0"
    ]
  }
}
